{
  "name": "iampersistent/psr11-symfony-console",
  "type": "library",
  "description": "make a psr11 container available in the application for a console",
  "keywords": ["psr11", "symfony", "console"],
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "psr/container": "^1.0",
    "symfony/console": "^4.0 || ^5.0""
  },
  "require-dev": {
  },
  "repositories": {
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "IamPersistent\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Fixture\\": "tests/_support/Fixture",
      "Tests\\Functional\\": "tests/functional",
      "Tests\\Unit\\": "tests/unit"
    }
  }
}
